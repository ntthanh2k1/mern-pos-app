model RefreshToken {
  refreshTokenId Int @id @default(autoincrement())
  userId Int
  token String @db.VarChar(1024)
  userAgent String 
  expiresAt  DateTime
  createdAt  DateTime @default(now())

  user User @relation(fields: [userId], references: [userId])

  @@index([token])
  @@index([expiresAt])
}